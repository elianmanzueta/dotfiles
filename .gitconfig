[user]
  name = Elian Manzueta
  email = eliandmanzueta@gmail.com
[color]
  ui = auto
[core]
  editor = code --wait
  whitespace = -trailing-space
[diff]
  mnemonicPrefix = true
  renames = true
  tool = vscode
[push]
  default = upstream
[pull]
  rebase = merges
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
